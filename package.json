  "bugs": {
    "url": "https://github.com/ksentak/mernshop/issues"
  },
  "homepage": "https://github.com/ksentak/mernshop#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.13"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
