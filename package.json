{
  "name": "mernshop",
  "version": "1.0.0",
  "description": "MERN e-commerce app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node back-end/server",
    "server": "npx nodemon back-end/server",
    "client": "npm start --prefix front-end",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
